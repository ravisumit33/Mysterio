{
  "addons": [
    "heroku-postgresql",
    "heroku-redis"
  ],
  "buildpacks": [
    {
      "url": "heroku/nodejs"
    },
    {
      "url": "heroku/python"
    }
  ],
  "env": {
    "DJANGO_SETTINGS_MODULE": {
      "required": true
    },
    "REDIS_TLS_URL": {
      "required": true
    },
    "REDIS_URL": {
      "required": true
    },
    "SECRET_KEY": {
      "required": true
    }
  },
  "formation": {
    "web": {
      "quantity": 1
    },
    "periodic": {
      "quantity": 1
    }
  },
  "name": "Mysterio",
  "scripts": {
  },
  "stack": "heroku-20"
}
